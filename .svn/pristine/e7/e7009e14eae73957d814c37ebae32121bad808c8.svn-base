  font-size: 1.2rem;
  -webkit-user-select: none;
  user-select: none;
  -webkit-touch-callout: none
}

.mint-indexlist-indicator {
  position: absolute;
  width: 50px;
  height: 50px;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  text-align: center;
  line-height: 50px;
  background-color: rgba(0, 0, 0, .7);
  border-radius: 5px;
  color: #fff;
